{
    "name": "fasyncphp/async",
    "description": "async/await implementation with structured concurrency.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fasync\\Async\\": "src/"
        },
        "files": [
            "src/Helpers/global_functions.php",
            "src/Helpers/namespace_functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Reymart A. Calicdan"
        }
    ],
    "require": {
        "php": "^8.2",
        "fasyncphp/event-loop": "dev-main",
        "fasyncphp/promise": "dev-main"
    },
    "require-dev": {
        "pestphp/pest": "^4.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}